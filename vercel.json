{
  "version": 2,
  "builds": [
    {
      "src": "server/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "server/(.*)",
      "dest": "server/index.js"
    },
    {
      "src": "client/(.*)",
      "dest": "index.html"
    }
  ]
}
